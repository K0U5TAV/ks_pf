{
  "hosting": {
    "predeploy": "npm install -g @ionic/cli && npm i && ionic serve",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
